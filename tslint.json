{
  
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "globals":{
      "wx":true
  },
  "rules":{
    "no-console": false,
    "ordered-imports": false,
    "rxjs-collapse-imports": true,
    "rxjs-pipeable-operators-only": true,
    "rxjs-no-static-observable-methods": true,
    "rxjs-proper-imports": true,
    "no-bitwise":false,
    "interface-name":false
  }
}